{
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["es2020", "dom"],
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "removeComments": true
  },
  "include": [
    "Example/lib",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}